<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%><%@ taglib prefix="sp" uri="http://mos.xinfan.com/"%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><!DOCTYPE html><html lang="en"><head>      <jsp:include page="../head.jsp"></jsp:include><link href="/jslib/uiadmin/lib/icheck/icheck.css" rel="stylesheet" type="text/css" /><link href="/jslib/uiadmin/lib/Hui-iconfont/1.0.1/iconfont.css" rel="stylesheet" type="text/css" /><link href="/jslib/uiadmin/lib/webuploader/0.1.5/webuploader.css" rel="stylesheet" type="text/css" /><script type="text/javascript" src="/jslib/uiadmin/lib/My97DatePicker/WdatePicker.js"></script> <script type="text/javascript" src="/jslib/uiadmin/lib/icheck/jquery.icheck.min.js"></script> <script type="text/javascript" src="/jslib/uiadmin/lib/Validform/5.3.2/Validform.min.js"></script> </head><body><form action="goods-fashion-save.jspx" method="post" enctype="multipart/form-data" class="form form-horizontal" id="form-submit">		<input type="hidden" name="id" value="${id}"  />		<div class="row cl">			<label class="form-label col-2"><span class="c-red">*</span>图片：</label>			<div class="formControls col-10">				<input type="file" class="input-text" required="required"  placeholder="" name="file">			</div>		</div>		<div class="row cl">			<div class="col-10 col-offset-2">				<button class="btn btn-primary radius" type="submit"><i class="Hui-iconfont">&#xe632;</i> 保存</button>			</div>		</div>				<div style="height: 100px;"></div></form><script type="text/javascript">$(function(){	$('.skin-minimal input').iCheck({		checkboxClass: 'icheckbox-blue',		radioClass: 'iradio-blue',		increaseArea: '20%'	});	});$(function(){	$("#form-submit").Validform({		tiptype:3,		callback:function(form){			form[0].submit();		}	});});</script></body></html>