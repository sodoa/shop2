<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%><%@ taglib prefix="sp" uri="http://mos.xinfan.com/"%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><!DOCTYPE html><html lang="en"><head><jsp:include page="../head.jsp"></jsp:include><script type="text/javascript" src="/jslib/uiadmin/lib/My97DatePicker/WdatePicker.js"></script><script type="text/javascript" src="/jslib/uiadmin/lib/datatables/1.10.0/jquery.dataTables.min.js"></script></head><body>	<nav class="breadcrumb">		<i class="Hui-iconfont">&#xe67f;</i> 首页 <span class="c-gray en">&gt;</span>用户管理 <span class="c-gray en">&gt;</span>分销排名		<a class="btn btn-success radius r mr-20" style="line-height: 1.6em; margin-top: 3px" href="javascript:location.replace(location.href);" title="刷新">			<i class="Hui-iconfont">&#xe68f;</i>		</a>	</nav>	<div class="pd-20">			<form action="customer-distriction-list.jspx">		<div class="text-c">			二级下线数：			<span class="select-box" style="width:100px">				<select class="select" name="type">					<option value="1" <c:if test="${type==1}">selected</c:if>  >本周</option>					<option value="2" <c:if test="${type==2}">selected</c:if>  >本月</option>				</select>			</span>			<button type="submit" class="btn btn-success radius" id="btn_search" name="">				<i class="Hui-iconfont">&#xe665;</i> 查询			</button>		</div>		<div class="mt-20">			<table class="table table-border table-bordered table-hover table-bg table-sort">				<thead>					<tr class="text-c">						<th>用户名称</th>						<th>帐号</th>						<th>一级代理数</th>						<th>二级代理数</th>					</tr>				</thead>				<tbody>					<c:forEach var="item" items="${list}">						<tr>						<td>${item.displayname}</td>						<td>${item.account}</td>						<td>${item.level1count}</td>						<td>${item.level2count}</td>									</tr>								</c:forEach>				</tbody>			</table>		</div>		</form>	</div></body></html>