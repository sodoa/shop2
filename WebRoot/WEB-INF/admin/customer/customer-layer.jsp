<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%><%@ taglib prefix="sp" uri="http://mos.xinfan.com/"%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><!DOCTYPE html><html lang="en"><head>   <jsp:include page="../head.jsp"></jsp:include></head><body><nav class="breadcrumb"><i class="Hui-iconfont">&#xe67f;</i> 首页 <span class="c-gray en">&gt;</span>用户管理<span class="c-gray en">&gt;</span>用户分销 <a class="btn btn-success radius r mr-20" style="line-height:1.6em;margin-top:3px" href="javascript:location.replace(location.href);" title="刷新" ><i class="Hui-iconfont">&#xe68f;</i></a></nav><div class="pd-20">	<div  style="border: 1px solid #eee;">一级代理</div>		<div class="mt-20"  style="margin: 0px;padding: 0px;">		<table class="table table-border table-bordered table-hover table-bg table-sort">			<thead>				<tr class="text-c">                         <th>用户ID</th>                         <th>用户名称</th>                         <th>帐号</th>                         <th>注册时间</th>                         <th>操作</th>	            </tr>			</thead>			<tbody>		          <c:forEach var="item" items="${list1}"> 						<tr class="text-c">                               <td>${item.customerId}</td>                               <td>${item.displayname}</td>                               <td>${item.account}</td>                               <td><sp:dateformat time="${item.regdate}"/></td>                               <td><a href="javascript:void(0);" onclick="member_show('${item.customerId}')">虚拟成单</a></td>	                   </tr>		          		         </c:forEach>                              				</tbody>		</table>	</div>	<div style="border: 1px solid #eee;">二级代理</div>	<div class="mt-20" style="margin: 0px;padding: 0px;">		<table class="table table-border table-bordered table-hover table-bg table-sort">			<thead>				<tr class="text-c">                         <th>用户ID</th>                         <th>用户名称</th>                         <th>帐号</th>                         <th>注册时间</th>                         <th>操作</th>	            </tr>			</thead>			<tbody>		          <c:forEach var="item" items="${list2}"> 						<tr class="text-c">                               <td>${item.customerId}</td>                               <td>${item.displayname}</td>                               <td>${item.account}</td>                               <td><sp:dateformat time="${item.regdate}"/></td>                               <td><a href="javascript:void(0);" onclick="member_show('${item.customerId}')">虚拟成单</a></td>	                   </tr>		          		         </c:forEach>                              				</tbody>		</table>	</div>	</div><script type="text/javascript">function member_show(id){	layer_show('虚拟单',"set-virtual-order.jspx?cid="+id,600,450);}</script></body></html>