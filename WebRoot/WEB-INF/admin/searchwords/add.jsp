<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%><%@ taglib prefix="sp" uri="http://mos.xinfan.com/"%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><!DOCTYPE html><html lang="en"><head>      <jsp:include page="../head.jsp"></jsp:include>   <link href="/jslib/uiadmin/lib/icheck/icheck.css" rel="stylesheet" type="text/css" /><script type="text/javascript" src="/jslib/uiadmin/lib/icheck/jquery.icheck.min.js"></script> <script type="text/javascript" src="/jslib/uiadmin/lib/Validform/5.3.2/Validform.min.js"></script>   <script type="text/javascript" src="/jslib/uiadmin/lib/My97DatePicker/WdatePicker.js"></script><script type="text/javascript" src="/jslib/uiadmin/lib/datatables/1.10.0/jquery.dataTables.min.js"></script></head><body><div class="pd-20">  <form action="#" method="post" class="form form-horizontal" id="form-user-add">    <div class="row cl">      <label class="form-label col-3"><span class="c-red">*</span>关键字：</label>      <div class="formControls col-5">        <input type="text" class="input-text"  datatype="*1-20" placeholder="" id="words" name="words" />      </div>    </div>    <div class="row cl">      <div class="col-9 col-offset-2">        <input class="btn btn-primary radius" type="submit" value="&nbsp;&nbsp;保存&nbsp;&nbsp;" id="btn_add">      </div>    </div>  </form></div><script type="text/javascript">$(function(){		$("#form-user-add").Validform({		tiptype:2,		callback:function(form){			//form[0].submit();						 $.ajax({type:"POST",	             url:"./words-save.jspx",	             data:{'words':$("#words").val()},	             dataType:"json",	             success:function(data){					if(data.result == 0){						//alert("删除成功");						parent.window.location.reload();						var index = parent.layer.getFrameIndex(window.name);						parent.layer.close(index);					}	             }			});			return false;			}		});			});</script></body></html>