<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%><%@ taglib prefix="sp" uri="http://mos.xinfan.com/"%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><!DOCTYPE html><html lang="en"><head>      <jsp:include page="../head.jsp"></jsp:include><script type="text/javascript" src="/jslib/uiadmin/lib/My97DatePicker/WdatePicker.js"></script><script type="text/javascript" src="/jslib/uiadmin/lib/datatables/1.10.0/jquery.dataTables.min.js"></script></head><body><nav class="breadcrumb"><i class="Hui-iconfont">&#xe67f;</i> 首页 <span class="c-gray en">&gt;</span>进货出单<span class="c-gray en">&gt;</span>订单列表 <a class="btn btn-success radius r mr-20" style="line-height:1.6em;margin-top:3px" href="javascript:location.replace(location.href);" title="刷新" ><i class="Hui-iconfont">&#xe68f;</i></a></nav><div class="pd-20">	<form action="purchase-list.jspx" method="post">	<div class="text-c" style="text-align: left;"> 日期范围：		<input value="${end}" type="text" onFocus="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm',alwaysUseStartDate:true})" name="enddate" id="datemax" class="input-text Wdate" style="width:180px;">		<button type="submit" class="btn btn-success radius" id="btn_search" name="" ><i class="Hui-iconfont">&#xe665;</i>查询</button>		<input id="btnPrint" type="button" value="打印" class="btn btn-success radius"  onclick="javascript:window.print();" />  	</div>	<div class="mt-20">		<div style="padding: 10 80;">共计<span style="color:red;font-size:18px;">${size}</span>单</div>		<c:forEach var="item" items="${list}">		<div style="margin-top:20px;margin-bottom: 20px;border: 1px solid green;padding: 5px;">		  <table class="table table-striped table-bordered table-hover " style="margin-bottom: 10px;">									<tbody>										<tr>											<td>定单号：</td>											<td>${item.order.orderId}</td>											<td>共计金额（元）：</td>											<td><b style="font-size: 20px; color: red;">${item.order.totalAmount}</b></td>										</tr>										<tr>											<td>定单日期：</td>											<td><sp:dateformat time="${item.order.orderDate}" /></td>											<td>定单状态：</td>											<td>												<sp:dict_label_tag type="order_state" value="${item.order.status}"></sp:dict_label_tag>											</td>										</tr>										<tr>											<td>支付方式：</td>											<td><sp:dict_label_tag type="payment_mode" value="${item.order.paymentMode}"></sp:dict_label_tag></td>											<td>是否短信确认</td>											<td><sp:dict_label_tag type="shared_state" value="${item.order.shared}"></sp:dict_label_tag></td>										</tr>									</tbody>			</table>													<table class="table table-striped table-bordered table-hover" style="margin-bottom: 10px;">									<tbody>										<tr>											<td>送货地址：</td>											<td>${item.order.receiverAddress}</td>										</tr>										<tr>											<td>收货人：</td>											<td>${item.order.receiverName}</td>										</tr>										<tr>											<td>联系电话</td>											<td>${item.order.receiverPhone}</td>										</tr>										<tr>											<td>送货说明</td>											<td><b style="font-size: 18px; color: green;">${item.order.mark}</b></td>										</tr>									</tbody>			</table>						<table class="table table-striped table-bordered table-hover">				<thead>					<tr>						<th>#</th>						<th>商品名称</th>						<th>数量（份）</th>						<th>单价（元）</th>					</tr>				</thead>				<tbody>					<c:forEach var="d" items="${item.detail}" varStatus="s">						<tr>							<td>${s.index+1}</td>							<td>${d.goodsName}【${d.weight}${d.unit}】</td>							<td>${d.quantity}</td>							<td>${d.finalPrice}</td>						</tr>					</c:forEach>				</tbody>			</table>								</div>				</c:forEach>	</div>	</form></div><script type="text/javascript">function order_detail(obj,id){	order_out(id);}function order_out(id){		var index = parent.layer.open({		type : 2,		title : '订单出货',		content : 'order-info.jspx?oid='+id	});	parent.layer.full(index);	}</script></body></html>