<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%><%@ taglib prefix="sp" uri="http://mos.xinfan.com/"%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><!DOCTYPE html><html lang="en"><head>      <jsp:include page="../head.jsp"></jsp:include><script type="text/javascript" src="/jslib/uiadmin/lib/My97DatePicker/WdatePicker.js"></script><script type="text/javascript" src="/jslib/uiadmin/lib/datatables/1.10.0/jquery.dataTables.min.js"></script></head><body><nav class="breadcrumb"><i class="Hui-iconfont">&#xe67f;</i> 首页 <span class="c-gray en">&gt;</span>进货出单<span class="c-gray en">&gt;</span>进货汇总 <a class="btn btn-success radius r mr-20" style="line-height:1.6em;margin-top:3px" href="javascript:location.replace(location.href);" title="刷新" ><i class="Hui-iconfont">&#xe68f;</i></a></nav><div class="pd-20">	<form action="purchase-summary.jspx" method="post">	<div class="text-c" style="text-align: left;"> 日期范围：		<input value="${end}" type="text" onFocus="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm',alwaysUseStartDate:true})" name="enddate" id="datemax" class="input-text Wdate" style="width:180px;">		<button type="submit" class="btn btn-success radius" id="btn_search" name="" ><i class="Hui-iconfont">&#xe665;</i>查询</button>		<input id="btnPrint" type="button" value="打印" class="btn btn-success radius"  onclick="javascript:window.print();" />  	</div>	<div class="mt-20">					<table class="table table-striped table-bordered table-hover">				<thead>					<tr>						<th>#</th>						<th>商品名称</th>						<th>合计数量（份）/单位</th>						<th>合计重量（斤）</th>						<th>合计</th>					</tr>				</thead>				<tbody>					<c:forEach var="d" items="${list}" varStatus="s">						<tr>							<td>${s.index+1}</td>							<td>${d.goodsName}</td>							<td>${d.count} / ${d.unit} </td>							<td>${d.weight}</td>							<td>${d.count}（${d.unit}）  x  ${d.weight}（斤）</td>						</tr>					</c:forEach>				</tbody>			</table>				</div>	</form></div></body></html>