<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%><%@ taglib prefix="sp" uri="http://mos.xinfan.com/"%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><!DOCTYPE html><html lang="en"><head>   <jsp:include page="../head.jsp"></jsp:include><link href="/jslib/uiadmin/lib/icheck/icheck.css" rel="stylesheet" type="text/css" /><link href="/jslib/uiadmin/lib/Hui-iconfont/1.0.1/iconfont.css" rel="stylesheet" type="text/css" /><link href="/jslib/uiadmin/lib/webuploader/0.1.5/webuploader.css" rel="stylesheet" type="text/css" /><script type="text/javascript" src="/jslib/uiadmin/lib/My97DatePicker/WdatePicker.js"></script> <script type="text/javascript" src="/jslib/uiadmin/lib/icheck/jquery.icheck.min.js"></script> <script type="text/javascript" src="/jslib/uiadmin/lib/Validform/5.3.2/Validform.min.js"></script> <script type="text/javascript" src="/jslib/uiadmin/lib/webuploader/0.1.5/webuploader.min.js"></script> <script type="text/javascript" src="/jslib/uiadmin/lib/ueditor/1.4.3/ueditor.config.js"></script><script type="text/javascript" src="/jslib/uiadmin/lib/ueditor/1.4.3/ueditor.all.min.js"> </script><script type="text/javascript" src="/jslib/uiadmin/lib/ueditor/1.4.3/lang/zh-cn/zh-cn.js"></script></head><body><div class="pd-20">	<form  class="form form-horizontal" id="form-submit">		<div class="row cl">			<label class="form-label col-2"><span class="c-red">*</span>产品标题：</label>			<div class="formControls col-10">				<input type="text" disabled="disabled" class="input-text" datatype="s2-30" placeholder="" id="goods_name" name="goods_name" value="${bean.goodsName}">			</div>		</div>		<div class="row cl">			<label class="form-label col-2"><span class="c-red">*</span>简略标题：</label>			<div class="formControls col-10">				<input type="text" class="input-text" disabled="disabled" datatype="s2-300" value="${bean.goodsLname}" placeholder="" id="goods_lname" name="goods_lname">			</div>		</div>		<div class="row cl">			<label class="form-label col-2"><span class="c-red">*</span>分类栏目：</label>			<div class="formControls col-4"> <span class="select-box">				<select name="type_level1" class="select" datatype="*1-100" disabled="disabled">						<option value="" >请选择</option>						<sp:goods_type_option_tag value="${bean.typeLevel1}"></sp:goods_type_option_tag>				</select>				</span> </div>			<label class="form-label col-2"><span class="c-red">*</span>排序值：</label>			<div class="formControls col-4">				<input type="text" class="input-text" disabled="disabled" value="${bean.sort}" datatype="n1-100" ignore="ignore" placeholder="" id="field_sort" name="sort">			</div>		</div>		<div class="row cl">			<label class="form-label col-2">产品属性：</label>			<div class="formControls col-4 skin-minimal">				<div class="check-box">					<label for="checkbox-hot">热销;</label>				    <input type="checkbox" disabled="disabled" id="checkbox-hot" name="hot" value="1" <c:if test="${bean.hot==1}">  checked="checked" </c:if> ></div>				    <div class="check-box">				    <label for="checkbox-burst">爆品;</label>				    <input type="checkbox" disabled="disabled" id="checkbox-burst" name="burst" value="1" <c:if test="${bean.burst==1}">  checked="checked" </c:if> > 				    </div>				    <div class="check-box">				    <label for="checkbox-fashion">营销;</label>				    <input type="checkbox" disabled="disabled" id="checkbox-fashion" name="fashion" value="1" <c:if test="${bean.fashion==1}">  checked="checked" </c:if> >					    </div>			    			</div>						<label class="form-label col-2">产品主题：</label>			<div class="formControls col-4">				<span class="select-box">				<select class="select" name="theme_type">					<sp:dict_option_tag type="goods_theme_type" value="${bean.themeType}">					</sp:dict_option_tag>				</select>				</span>			</div>						</div>		<div class="row cl">			<label class="form-label col-2">宣传页面：</label>			<div class="formControls col-10">				<input type="text"  disabled="disabled" name="fashion_template" id="fashion_template" value="${bean.fashionTemplate}" class="input-text">			</div>		</div>		<div class="row cl">			<label class="form-label col-2">产地：</label>			<div class="formControls col-4">				<input type="text" name="goods_area" disabled="disabled" id="goods_area" placeholder="" value="${bean.goodsArea}" class="input-text">			</div>			<label class="form-label col-2">所属供应商：</label>			<div class="formControls col-4">				<input type="text" name="supplier" id="supplier"  disabled="disabled" placeholder="" value="${bean.supplier}" class="input-text">			</div>		</div>		<div class="row cl">			<label class="form-label col-2">产品重量：</label>			<div class="formControls col-4">				<input type="text" name="" id="" placeholder="" value="" disabled="disabled" class="input-text" style="width:90%">				</div>			<label class="form-label col-2">包装单位：</label>			<div class="formControls col-4"> <span class="select-box">				<select class="select" name="unit" disabled="disabled">					<option>请选择</option>					<sp:dict_option_tag type="goods_unit" value="${bean.unit}"></sp:dict_option_tag>				</select>				</span> </div>		</div>		<div class="row cl">			<label class="form-label col-2"><span class="c-red">*</span>产品展示价格：</label>			<div class="formControls col-4">				<input type="text" name="final_prices" id="final_prices" disabled="disabled" ignore="ignore" placeholder="" datatype="n1-1000" value="${bean.finalPrices}" class="input-text" style="width:90%">				元</div>			<label class="form-label col-2"><span class="c-red">*</span>市场价格：</label>			<div class="formControls col-4">				<input type="text" name="orgin_prices" id="orgin_prices" disabled="disabled" ignore="ignore" placeholder="" datatype="n1-1000" value="${bean.orginPrices}" class="input-text" style="width:90%">				元</div>		</div>		<div class="row cl">			<label class="form-label col-2">产品关键字：</label>			<div class="formControls col-10">				<input type="text" name="keywords" id="keywords" disabled="disabled" placeholder="多个关键字用英文逗号隔开，限10个关键字"  value="${bean.keywords}"  class="input-text">			</div>		</div>		<div class="row cl">			<label class="form-label col-2"><span class="c-red">*</span>产品摘要：</label>			<div class="formControls col-10">				<textarea id="goods_des" name="goods_des" cols="" rows="" disabled="disabled" class="textarea"  placeholder="说点什么...最少输入10个字符" datatype="*10-2000" dragonfly="true" nullmsg="备注不能为空！" onKeyUp="textarealength(this,2000)">${bean.goodsDes} </textarea>				<p class="textarea-numberbar"></p>			</div>		</div>		<div class="row cl">			<label class="form-label col-2">缩略图：</label>			<div class="formControls col-10">				<div class="uploader-thum-container">					<div id="uploader-demo" class="wu-example">					    <div id="fileList" class="uploader-list">					    	<img src="${bean.thumbnailUrl}"/>					    </div>					 </div>					</div>				</div>			</div>				<div class="row cl">			<label class="form-label col-2">详细内容：</label>			<div class="formControls col-10"> 				${html}			</div>		</div>				</form>		</div><script type="text/javascript">$(function(){	$('.skin-minimal input').iCheck({		checkboxClass: 'icheckbox-blue',		radioClass: 'iradio-blue',		increaseArea: '20%'	});	});</script></body></html>