<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%><%@ taglib prefix="sp" uri="http://mos.xinfan.com/"%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><!DOCTYPE html><html lang="en"><head>      <jsp:include page="../head.jsp"></jsp:include><script type="text/javascript" src="/jslib/uiadmin/lib/My97DatePicker/WdatePicker.js"></script><script type="text/javascript" src="/jslib/uiadmin/lib/datatables/1.10.0/jquery.dataTables.min.js"></script></head><body><nav class="breadcrumb"><i class="Hui-iconfont">&#xe67f;</i> 首页 <span class="c-gray en">&gt;</span>商品管理<span class="c-gray en">&gt;</span>Fashion图片 <a class="btn btn-success radius r mr-20" style="line-height:1.6em;margin-top:3px" href="javascript:location.replace(location.href);" title="刷新" ><i class="Hui-iconfont">&#xe68f;</i></a></nav><div class="pd-20">	<div class="cl pd-5 bg-1 bk-gray mt-20">  <a class="btn btn-primary radius" onclick="add('${id}')" href="javascript:;"><i class="Hui-iconfont">&#xe600;</i> 添加</a></span> </div>	<div class="mt-20">		<div style="float: left;">		<c:forEach var="item" items="${list}">			<div style="margin: 10px;float: left;">				<img alt="" src="${item.imageUrl}"  width="200" height="200">				<div style="text-align: center;">					<a href="javascript:;" onclick="pre('${item.gImageId}')">上移</a>					<a href="javascript:;" onclick="del('${item.gImageId}')">删除</a>					<a href="javascript:;" onclick="next('${item.gImageId}')">下移</a>				</div>			</div>		</c:forEach>		</div>	</div></div><script type="text/javascript">function add(id){	var index = layer.open({		type: 2,		title: '添加照片',		content: 'goods-fashion-add.jspx?id='+id	});	layer.full(index);}function pre(id){	sort(id,1);}function next(id){	sort(id,0);}function sort(id,type){		  var _data = {"id": id,"type":type};      $.ajax({type:"POST",             url:"goods-fashion-sort.jspx?t="+new Date().getTime(),             data:_data,             dataType:"json",             success:function(data){            	if(data.result ==0){            		layer.msg('操作成功!',{icon:1,time:1000});            		location.replace(location.href);            	}            	else{            		layer.msg(data.message,{icon:1,time:1000});            	}             }	  });}function del(id){	layer.confirm('确认要删除吗？',function(index){	  var _data = {"id": id};      $.ajax({type:"POST",             url:"goods-fashion-del.jspx?t="+new Date().getTime(),             data:_data,             dataType:"json",             success:function(data){            	if(data.result ==0){            		layer.msg('操作成功!',{icon:1,time:1000});            		location.replace(location.href);            	}            	else{            		layer.msg(data.message,{icon:1,time:1000});            	}             }		});	});}</script></body></html>