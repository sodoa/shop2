<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%><%@ taglib prefix="sp" uri="http://mos.xinfan.com/"%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><!DOCTYPE html><html lang="en"><head><jsp:include page="../head.jsp"></jsp:include><script type="text/javascript" src="/jslib/uiadmin/lib/Validform/5.3.2/Validform.min.js"></script> </head><body><div class="pd-20">	<form action="post-virtual-order.jspx" method="post" class="form form-horizontal" id="form-submit">		<input type="hidden" name="customerId" value="${customerId}"  id="customerId"/>		<div class="row cl">			<label class="form-label col-2"><span class="c-red">*</span>定单标题：</label>			<div class="formControls col-10">				<input type="text" class="input-text" value="" datatype="*2-100" placeholder="" id="chargeName" name="chargeName">			</div>		</div>		<div class="row cl">			<label class="form-label col-2"><span class="c-red">*</span>价格：</label>			<div class="formControls col-10">				<input type="text" class="input-text" datatype="n2-200" value="" placeholder="" id="amount" name="amount">			</div>		</div>		<div class="row cl">			<div class="col-10 col-offset-2">				<button class="btn btn-primary radius" type="submit"><i class="Hui-iconfont">&#xe632;</i>提交 </button>			</div>		</div>			</form></div><script type="text/javascript">$(function(){	$("#form-submit").Validform({		ajaxPost : true,		tiptype : 4,		callback : function(data) {			if (data.result == 0) {				layer.msg('修改成功');								window.setTimeout(function(){					var index = parent.layer.getFrameIndex(window.name);					parent.layer.close(index);				},"1000");								return true;			} else {				layer.msg(data.message);				return false;			}		},		tiptype : function(msg, o, cssctl) {			if(o.type == 3){				layer.msg(msg);			}		}	});});</script></body></html>